{
  "manifest_version": 3,
  "name": "ChroMarx",
  "version": "1.0.0",
  "description": "A beautiful and functional Chrome extension",
  "permissions": [
    "sidePanel",
    "bookmarks",
    "contextMenus",
    "notifications",
    "storage",
    "alarms"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "ChroMarx",
    "default_icon": {
      "16": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png",
      "48": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png",
      "128": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png"
    }
  },
  "icons": {
    "16": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png",
    "48": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png",
    "128": "lovable-uploads/95959624-8b98-4a44-b8d1-393d07c243bc.png"
  }
}